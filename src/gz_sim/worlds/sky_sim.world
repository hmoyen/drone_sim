      </link>
    </model>

    <include>
      <uri>model://sun</uri>
    </include>

    <model name="iris">
      <include>
        <uri>model://drone_with_camera_down</uri>
      </include>
        <pose> 0 0 0.05 0 0 0</pose>
    </model>
    
    <model name="aruquinho">
      <include>
        <uri>model://aruco_small</uri>
      </include>
      <pose> 2 2 0 0 0 0</pose>
    </model>

     <model name="box">
      <link name="link">
        <pose>1 1 50 0 0 0</pose>

        <collision name="box_collision">
          <geometry>
            <box>
              <size>1 1 100</size>
            </box>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <box>
              <size>1 1 100</size>
            </box>
          </geometry>
        </visual>
      </link>

    <static>false</static>
    <turnGravityOff>true</turnGravityOff>
    </model>


